{
    "name": "speedybot-starter",
    "version": "1.0.0",
    "description": "Starter template for speedybot",
    "main": "./dist/src/index.js",
    "bin": {
        "helper": "./dist/src/cli/index.js"
    },
    "scripts": {
        "dev": "nodemon",
        "start:server": "ts-node src/server",
        "start:webhook": "npm run start:server",
        "start": "ts-node src/launcher",
        "start:dist": "npm run setup && cd dist && node src/launcher",
        "build": "./node_modules/typescript/bin/tsc",
        "setup": "npm i && npm run build",
        "write:json": "cd src/util && ts-node index"
    },
    "keywords": [
        "bot",
        "scaffold",
        "demo",
        "speedybot"
    ],
    "author": "valgaze@gmail.com",
    "license": "MIT",
    "files": [
        "dist/**"
    ],
    "repository": {
        "type": "git",
        "url": "https://github.com/valgaze/speedybot.git"
    },
    "devDependencies": {
        "nodemon": "^2.0.12",
        "ts-node": "^8.10.1",
        "typescript": "^3.8.3"
    },
    "dependencies": {
        "@types/node": "^16.9.1",
        "body-parser": "^1.19.0",
        "express": "^4.17.1",
        "speedybot": "^1.0.5",
        "webex-node-bot-framework": "^2.3.7"
    }
}
